/*CSS动画*/
@keyframes bounceInUiScale
  from
    opacity 0
    transform scale(.9)
  to
    opacity 1
    transform scale(1)
@keyframes bounceOutUiScale
  from
    opacity 1
    transform scale(1)
  to
    opacity 0
    transform scale(.9)

.bounceInUiOutScale-enter-active
  animation bounceInUiScale .3s ease-out
.bounceInUiOutScale-leave-active
  animation bounceOutUiScale .3s ease-in
