{
	"name": "double-ui-vue",
	"version": "0.0.4",
	"description": "基于vue框架的ui库",
	"private": false,
	"author": "xiongshuang<https://github.com/xiongshuang>",
	"repository": "https://github.com/xiongshuang/double-ui-vue.git",
	"keywords": [
		"vue3",
		"ui",
		"PC端",
		"ui框架",
		"component"
	],
	"license": "MIT",
	"main": "es/index.js",
	"module": "es/index.js",
	"types": "es/index.d.ts",
	"files": [
		"es/*"
	],
	"sideEffects": [
		"src/assets/stylus/main.styl",
		"src/components/base/*",
		"es/*",
		"*.ts",
		"*.tsx",
		"*.styl",
		"*.css"
	],
	"scripts": {
		"clean": "rm -rf es",
		"clean:node_modules": "rm -rf node_modules",
		"dev": "webpack serve --mode development --config config/dev.js",
		"prod": "webpack --mode production --config config/prod.js",
		"generate:index": "go run config/generateIndex.go",
		"base:css": "node config/baseCss.js",
		"prebuild": "npm run clean",
		"build": "npm run base:css && node config/build.js && npm run generate:index",
		"lint": "eslint src --fix",
		"vite:dev": "vite --config config/vite.config.ts",
		"test": "jest",
		"docs:dev": "vitepress dev docs --host --port 3004",
		"docs:build": "vitepress build docs",
		"docs:serve": "vitepress serve docs",
		"serve": "node server.js"
	},
	"devDependencies": {
		"@babel/core": "^7.18.5",
		"@babel/plugin-transform-modules-commonjs": "^7.18.2",
		"@babel/plugin-transform-runtime": "^7.18.5",
		"@babel/preset-env": "^7.18.2",
		"@babel/preset-typescript": "^7.17.12",
		"@babel/runtime-corejs3": "^7.18.3",
		"@types/jest": "^28.1.3",
		"@types/webpack-env": "^1.17.0",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"@vitejs/plugin-vue": "^2.3.3",
		"@vitejs/plugin-vue-jsx": "^1.3.10",
		"@vue/babel-plugin-jsx": "^1.1.1",
		"@vue/compiler-sfc": "^3.2.37",
		"@vue/test-utils": "^2.0.0",
		"autoprefixer": "^10.4.7",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^28.1.1",
		"babel-loader": "^8.2.5",
		"clean-webpack-plugin": "^4.0.0",
		"core-js": "^3.23.2",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^4.0.0",
		"eslint": "^8.18.0",
		"eslint-plugin-babel": "^5.3.1",
		"eslint-plugin-compat": "^4.0.2",
		"eslint-plugin-html": "^6.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-n": "^15.2.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-standard": "^5.0.0",
		"eslint-plugin-vue": "^9.1.1",
		"html-webpack-plugin": "^5.5.0",
		"jest": "^28.1.1",
		"jest-transform-stub": "^2.0.0",
		"js-func-tools": "^1.0.9",
		"mini-css-extract-plugin": "^2.6.1",
		"postcss": "^8.4.14",
		"postcss-loader": "^7.0.0",
		"postcss-nested": "^5.0.6",
		"style-loader": "^3.3.1",
		"stylus": "^0.58.1",
		"stylus-loader": "^7.0.0",
		"svg-component-loader": "^0.0.1",
		"terser-webpack-plugin": "^5.3.3",
		"ts-jest": "^28.0.5",
		"typescript": "^4.7.4",
		"vite": "^2.9.13",
		"vite-plugin-dts": "^1.2.1",
		"vite-svg-loader": "^3.4.0",
		"vitepress": "1.0.0-alpha.4",
		"vue": "^3.2.37",
		"vue-jest": "^5.0.0-alpha.8",
		"vue-loader": "^17.0.0",
		"vue-router": "^4.0.16",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.9.2",
		"webpack-merge": "^5.8.0"
	}
}
