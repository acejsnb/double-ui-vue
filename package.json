{
	"name": "double-ui-vue",
	"version": "0.0.2",
	"description": "基于vue框架的ui库",
	"private": false,
	"author": "xiongshuang",
	"keywords": [
		"vue3",
		"ui",
		"PC端",
		"ui框架",
		"component"
	],
	"license": "MIT",
	"main": "lib/index.js",
	"files": [
		"lib/*"
	],
	"sideEffects": [
		"src/assets/stylus/main.styl",
		"src/components/base/*",
		"lib/*",
		"*.ts",
		"*.tsx",
		"*.styl",
		"*.css"
	],
	"scripts": {
		"clean": "rimraf ./lib",
		"clean:node_modules": "rimraf ./node_modules",
		"dev": "webpack serve --mode development --config ./config/dev.js",
		"prod": "webpack --mode production --config ./config/prod.js",
		"full": "webpack --mode production --config ./config/full.js",
		"create:index": "node ./config/createIndex.js",
		"single": "npm run clean && webpack --mode production --config ./config/single.js && npm run create:index",
		"baseCss": "webpack --mode production --config ./config/baseCss.js",
		"build": "npm run clean && npm run baseCss && npm run full && npm run single",
		"lint": "eslint src --fix",
		"prettier": "npx prettier --write .",
		"vite": "vite",
		"test": "jest"
	},
	"devDependencies": {
		"@babel/core": "^7.15.8",
		"@babel/plugin-transform-modules-commonjs": "^7.15.4",
		"@babel/plugin-transform-runtime": "^7.15.8",
		"@babel/preset-env": "^7.15.8",
		"@babel/preset-typescript": "^7.15.0",
		"@babel/runtime-corejs3": "^7.15.4",
		"@types/jest": "^26.0.23",
		"@types/webpack-env": "^1.16.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^4.22.0",
		"@vitejs/plugin-vue": "^1.2.1",
		"@vitejs/plugin-vue-jsx": "^1.1.3",
		"@vue/babel-plugin-jsx": "^1.0.6",
		"@vue/cli-plugin-babel": "^4.5.14",
		"@vue/compiler-sfc": "^3.1.1",
		"@vue/eslint-config-standard": "^6.1.0",
		"@vue/eslint-config-typescript": "^8.0.0",
		"@vue/test-utils": "^2.0.0-alpha.0",
		"autoprefixer": "^10.2.4",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.6.3",
		"babel-loader": "^8.2.2",
		"chalk": "^4.1.1",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^8.1.0",
		"core-js": "^3.18.3",
		"css-loader": "^6.4.0",
		"css-minimizer-webpack-plugin": "^3.1.1",
		"cssnano": "^4.1.10",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.2.0",
		"eslint-plugin-babel": "^5.3.1",
		"eslint-plugin-compat": "^3.9.0",
		"eslint-plugin-html": "^6.2.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.1.0",
		"eslint-plugin-standard": "^5.0.0",
		"eslint-plugin-vue": "^7.20.0",
		"html-webpack-plugin": "^5.3.1",
		"jest": "^26.6.3",
		"jest-transform-stub": "^2.0.0",
		"js-func-tools": "^0.0.6",
		"mini-css-extract-plugin": "2.4.2",
		"postcss": "^8.2.6",
		"postcss-loader": "^5.0.0",
		"precss": "^4.0.0",
		"prettier": "^2.2.1",
		"progress-bar-webpack-plugin": "^2.1.0",
		"style-loader": "^3.3.1",
		"stylus": "^0.54.8",
		"stylus-loader": "^5.0.0",
		"terser-webpack-plugin": "^5.1.1",
		"ts-jest": "^26.5.5",
		"ts-loader": "^9.1.1",
		"typescript": "^4.2.4",
		"vite": "^2.3.7",
		"vite-svg-loader": "^1.5.1",
		"vue": "^3.1.1",
		"vue-jest": "^5.0.0-alpha.8",
		"vue-loader": "^16.2.0",
		"vue-router": "^4.0.6",
		"vuecomponent-svg-loader": "^0.0.8",
		"webpack": "^5.59.1",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.3.1",
		"webpack-merge": "^5.8.0"
	}
}
