{
	"name": "double-ui-vue",
	"version": "0.0.2",
	"description": "基于vue框架的ui库",
	"private": false,
	"author": "xiongshuang<https://github.com/xiongshuang>",
	"repository": "https://github.com/xiongshuang/double-ui-vue.git",
	"keywords": [
		"vue3",
		"ui",
		"PC端",
		"ui框架",
		"component"
	],
	"license": "MIT",
	"main": "lib/index.js",
	"files": [
		"lib/*"
	],
	"sideEffects": [
		"src/assets/stylus/main.styl",
		"src/components/base/*",
		"lib/*",
		"*.ts",
		"*.tsx",
		"*.styl",
		"*.css"
	],
	"scripts": {
		"clean": "rm -rf ./lib",
		"clean:node_modules": "rimraf ./node_modules",
		"dev": "webpack serve --mode development --config ./config/dev.js",
		"prod": "webpack --mode production --config ./config/prod.js",
		"create:index": "node ./config/createIndex.js",
		"single": "webpack --mode production --config ./config/single.js && npm run create:index",
		"baseCss": "webpack --mode production --config ./config/baseCss.js",
		"build": "npm run clean && npm run baseCss && npm run single",
		"build:vite": "npm run clean && npm run baseCss && node ./config/viteBuild.js && npm run create:index",
		"lint": "eslint src --fix",
		"vite": "vite",
		"test": "jest",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:serve": "vitepress serve docs"
	},
	"devDependencies": {
		"@babel/core": "^7.17.10",
		"@babel/plugin-transform-modules-commonjs": "^7.17.9",
		"@babel/plugin-transform-runtime": "^7.17.10",
		"@babel/preset-env": "^7.17.10",
		"@babel/preset-typescript": "^7.16.7",
		"@babel/runtime-corejs3": "^7.17.9",
		"@types/jest": "^27.5.1",
		"@types/webpack-env": "^1.16.4",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"@vitejs/plugin-vue": "^2.3.3",
		"@vitejs/plugin-vue-jsx": "^1.3.10",
		"@vue/babel-plugin-jsx": "^1.1.1",
		"@vue/cli-plugin-babel": "^5.0.4",
		"@vue/compiler-sfc": "^3.2.33",
		"@vue/eslint-config-standard": "^6.1.0",
		"@vue/eslint-config-typescript": "^10.0.0",
		"@vue/test-utils": "^2.0.0-rc.21",
		"autoprefixer": "^10.4.7",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^28.1.0",
		"babel-loader": "^8.2.5",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^10.2.4",
		"core-js": "^3.22.5",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"cssnano": "^5.1.7",
		"eslint": "^8.15.0",
		"eslint-plugin-babel": "^5.3.1",
		"eslint-plugin-compat": "^4.0.2",
		"eslint-plugin-html": "^6.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-standard": "^5.0.0",
		"eslint-plugin-vue": "^8.7.1",
		"html-webpack-plugin": "^5.5.0",
		"jest": "^28.1.0",
		"jest-transform-stub": "^2.0.0",
		"js-func-tools": "^1.0.5",
		"mini-css-extract-plugin": "^2.6.0",
		"postcss": "^8.4.13",
		"postcss-loader": "^6.2.1",
		"precss": "^4.0.0",
		"style-loader": "^3.3.1",
		"stylus": "^0.57.0",
		"stylus-loader": "^6.2.0",
		"svg-component-loader": "^0.0.1",
		"terser-webpack-plugin": "^5.3.1",
		"ts-jest": "^28.0.2",
		"typescript": "^4.6.4",
		"vite": "^2.9.9",
		"vite-svg-loader": "^3.3.0",
		"vue": "^3.2.33",
		"vue-jest": "^5.0.0-alpha.8",
		"vue-loader": "^17.0.0",
		"vue-router": "^4.0.15",
		"vuecomponent-svg-loader": "^0.1.1",
		"webpack": "^5.72.1",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.9.0",
		"webpack-merge": "^5.8.0"
	}
}
